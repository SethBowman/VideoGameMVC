@model Game

<h3>@Model.Title Information</h3>

<table class="table">
    <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Genre</th>
        <th>Release Year</th>
        <th>Platform</th>      
    </tr>
    <tr>
        <td>@Model.Id</td>
        <td>@Model.Title</td>
        <td>@Model.Genre</td>
        <td>@Model.Release_Year</td>
        <td>@Model.Platform</td>       
    </tr>
</table>